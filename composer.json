{
    "name": "ddlzz/bisect",
    "description": "A library that solves problems similar to the python bisect module",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ddlzz\\Bisect\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andrew Alyamovsky",
            "email": "yankeedudlizz@gmail.com"
        }
